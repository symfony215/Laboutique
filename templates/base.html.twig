    <div class="container marketing  {% if block('carousel') is not defined %}mt-2{% endif %}">

        {% block content %}

        {% endblock %}

    </div><!-- /.container -->


    <!-- FOOTER -->
    {% include './layout/_footer.html.twig'%}
</main>
